new_Fac [0]
BEGIN
	MOVE TEMP 0 HALLOCATE 4
	MOVE TEMP 1 HALLOCATE 4
	HSTORE TEMP 0 0 TEMP 1
	MOVE TEMP 2 PLUS TEMP 1 0
	MOVE TEMP 3 Fac_ComputeFac
	HSTORE TEMP 2 0 TEMP 3
RETURN
	TEMP 0
END

