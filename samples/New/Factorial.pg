Fac_ComputeFac [2]
BEGIN
	MOVE TEMP 27 TEMP 1
	MOVE TEMP 28 1
	MOVE TEMP 26 LT TEMP 27 TEMP 28
	CJUMP TEMP 26 L1
	L0
		MOVE TEMP 30 1
		MOVE TEMP 29 TEMP 30
		MOVE TEMP 25 TEMP 29
		JUMP L2
	L1
		MOVE TEMP 32 TEMP 1
		MOVE TEMP 35 TEMP 0
		MOVE TEMP 37 TEMP 1
		MOVE TEMP 38 1
		MOVE TEMP 36 MINUS TEMP 37 TEMP 38
		HLOAD TEMP 39 TEMP 35 0
		HLOAD TEMP 39 TEMP 39 0
		MOVE TEMP 34 CALL TEMP 39 ( TEMP 35 TEMP 36 )
MOVE TEMP 33 TEMP 34		MOVE TEMP 31 TIMES TEMP 32 TEMP 33
		MOVE TEMP 25 TEMP 31
	L2	NOOP
	MOVE TEMP 41 TEMP 25
	MOVE TEMP 40 TEMP 41
